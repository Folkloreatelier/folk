{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    plugins: [
        "import"
    ],
    "rules": {
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "import/namespace": ["error", { allowComputed: true }]
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true
    }
}
